{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "us-phone-number-generator",
  "title": "US Phone Number Generator",
  "description": "Generate Random US Phone Number",
  "icon": "command-icon.png",
  "author": "anze_dragar",
  "categories": [
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Random phone number",
      "description": "Generate a random phone number and store it in clipboard",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.75.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "@types/node": "^20.14.2",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}